{
  "name": "@appland/appmap",
  "version": "0.2.3",
  "main": "dist/appmap.esm.js",
  "module": "dist/appmap.esm.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
    "watch": "cross-env NODE_ENV=production rollup --watch --config rollup.config.js --format es",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "cypress run",
    "test": "npm run test:unit && server-test 'npm run storybook -- --ci --quiet' :6006 'npm run test:e2e -- --headless --record false'",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes",
    "cypress": "cypress open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm test"
    }
  },
  "files": [
    "dist/appmap.esm.js"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "d3": "^5.16.0",
    "d3-flame-graph": "^4.0.6",
    "dagre": "^0.8.5",
    "deepmerge": "^4.2.2",
    "sql-formatter": "^2.3.3",
    "vue": "^2.6.11",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-replace": "^2.3.2",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/vue": "^6.1.14",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.5.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "geometry-interfaces": "^1.1.4",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mathjs": "^9.0.0",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.1.1",
    "start-server-and-test": "^1.11.7",
    "vue-template-compiler": "^2.6.11"
  },
  "lint-staged": {
    "*.{js,md,json,vue}": [
      "prettier --write"
    ]
  }
}
