{
  "name": "json-schema-diff",
  "version": "0.17.1",
  "description": "A language agnostic CLI tool and nodejs api to identify differences between two json schema files.",
  "bin": {
    "json-schema-diff": "./bin/json-schema-diff"
  },
  "main": "dist/api.js",
  "scripts": {
    "release-major": "./node_modules/.bin/gulp release --type major",
    "release-minor": "./node_modules/.bin/gulp release --type minor",
    "release-patch": "./node_modules/.bin/gulp release --type patch",
    "test": "./node_modules/.bin/gulp",
    "watch": "./node_modules/.bin/gulp watch",
    "watch-debug": "JSON_SCHEMA_DIFF_ENABLE_DEBUG=true ./node_modules/.bin/gulp watch",
    "watch-e2e": "./node_modules/.bin/gulp watchE2e"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/json-schema-diff.git"
  },
  "keywords": [
    "diff",
    "difference",
    "json",
    "schema",
    "json-schema"
  ],
  "author": {
    "name": "Ben Sayers (Atlassian)",
    "email": "bsayers@atlassian.com"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jasmine": "^3.6.1",
    "@types/json-schema": "^7.0.6",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12",
    "@types/verror": "^1.10.4",
    "ansi-colors": "^4.1.1",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-conventional-changelog": "^2.0.35",
    "gulp-filter": "^7.0.0",
    "gulp-git": "^2.10.1",
    "gulp-jasmine": "^4.0.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^5.0.1",
    "minimist": "^1.2.5",
    "run-sequence": "^2.2.1",
    "tslint": "6.1.3",
    "typescript": "4.3.4"
  },
  "types": "lib/api-types.d.ts",
  "dependencies": {
    "ajv": "^8.6.0",
    "commander": "^7.2.0",
    "json-schema-ref-parser": "^9.0.6",
    "json-schema-spec-types": "^0.1.2",
    "lodash": "^4.17.20",
    "verror": "^1.10.0"
  }
}
