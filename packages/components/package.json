{
  "name": "@appland/components",
  "version": "1.0.1",
  "main": "dist/appmap.node.js",
  "module": "dist/appmap.esm.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "cli": "node src/cli.js",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
    "watch": "cross-env NODE_ENV=production rollup --watch --config rollup.config.js --format es",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "cypress run",
    "test": "npm run test:unit && server-test 'npm run storybook -- --ci --quiet' :6006 'npm run test:e2e -- --headless --record false'",
    "lint": "eslint --ext .js --ext .vue src",
    "pre-commit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes --exit-once-uploaded",
    "cypress": "cypress open"
  },
  "lint-staged": {
    "*.{js,md,json,vue}": [
      "prettier --write"
    ]
  },
  "bin": {
    "appmap-js": "src/cli.js"
  },
  "files": [
    "dist/appmap.esm.js"
  ],
